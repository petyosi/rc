{
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "suggest.noselect": false,
  "jest.watchman": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": false,
  "explorer.icon.source": "vim-devicons",
  "explorer.file.hiddenRules": {
    "extensions": ["o", "a", "obj", "pyc"],
    "filenames": ["node_modules", "dist", ".tgz"],
    "patternMatches": ["^\\."]
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "css",
    "javascript",
    "javascriptreact",
    "json",
    "Json",
    "JSON",
    "rust",
    "scss",
    "typescript",
    "typescriptreact"
  ],
  "languageserver": {},
  "tsserver.useLocalTsdk": true,
  "tsserver.log": "off",
  "tsserver.trace.server": "off",
  "stylelintplus.autoFixOnSave": true,
  "stylelintplus.autoFixOnFormat": true,
  "snippets.ultisnips.enable": false,
  "snippets.loadVSCodeProjectSnippets": false,
  "snippets.excludePatterns": [
    "**/*jquery.snippets",
    "**/*redux.snippets",
    "**/*bemjson.snippets",
    "**/*jasmine.snippets",
    "**/*openui5.snippets"
  ]
}
